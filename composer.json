{
    "name": "laravel-interaction/rate",
    "description": "User rate/unrate behaviour for Laravel.",
    "keywords": ["laravel", "rating", "rate"],
    "license": "MIT",
    "authors": [
        {
            "name": "zingimmick",
            "email": "zingimmick@outlook.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/database": "^8.0 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0",
        "laravel-interaction/support": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.3 || ^1.4.2",
        "orchestra/testbench": "^6.0 || ^7.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelInteraction\\Rate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelInteraction\\Rate\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelInteraction\\Rate\\RateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
